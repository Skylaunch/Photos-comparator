<Window x:Class="Photos_comparator.Windows.User.ProgressBarWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Photos_comparator.Windows.User"
        mc:Ignorable="d"
        Title="Процесс поиска" Height="60" Width="500" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <ProgressBar Name="ProgressBar" Maximum="{Binding MaxValue}" Value="0"/>
    </Grid>
</Window>
